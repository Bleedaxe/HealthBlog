@page
@model IndexModel

<h2 class="text-center m-3">Стани треньор</h2>

<form method="post" enctype="multipart/form-data">

	<label asp-for="SertificateFile">Качи сертификат за треньор</label>
	<input type="file" asp-for="SertificateFile" class="form-control-file"/>

	<div class="mt-3">
		<input type="submit" value="Качи" class="btn btn-success" />
		<a asp-area=""
		   asp-controller="@ControllerConstants.Home"
		   asp-action="@ActionConstants.Index"
		   class="btn btn-light">Назад</a>
	</div>
</form>